{
  "name": "pokemon-challenge",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "regjsgen": "^0.4.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/template/scss/main.scss -o src/template",
    "watch-css": "npm run build-css && node-sass-chokidar src/template/scss -o  public/testebuild --watch --recursive",
    "dev": "webpack-dev-server --progress --colors --inline --hot",
    "prod": "webpack --progress -p",
    "start": "node server.js",
    "start-css": "npm-run-all -p watch-css start",
    "build": "npm-run-all build-css"
  },
  "devDependencies": {
    "axios": "^0.15.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bootstrap": "^3.3.7",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "jest": "^22.4.3",
    "node-sass-chokidar": "^1.2.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.7",
    "react-router": "^3.0.2",
    "redux": "^3.7.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
